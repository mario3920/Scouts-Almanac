<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.crud.CancioneiroController">
   <top>
      <Pane prefHeight="400.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="407.0" layoutY="344.0" mnemonicParsing="false" text="Cadastrar" />
            <Button fx:id="btnEditar" layoutX="338.0" layoutY="344.0" mnemonicParsing="false" onAction="#btnEditClick" text="Editar" />
            <GridPane fx:id="gridClientes" layoutX="7.0" layoutY="7.0" maxHeight="300.0" maxWidth="800.0" minHeight="300.0" minWidth="800.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
            <Button layoutX="252.0" layoutY="344.0" mnemonicParsing="false" onAction="#updateTableView" text="Atualizar" />
            <Button fx:id="btnRemover" layoutX="500.0" layoutY="344.0" mnemonicParsing="false" onAction="#btnRemoveClick" text="Remover" />
         </children>
      </Pane>
   </top>
</BorderPane>
